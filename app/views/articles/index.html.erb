<h1>Listing articles</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
    <td><%= article.title %></td>
    <td><%= article.text %></td>
  <% end %>
    </tr>

</table>
